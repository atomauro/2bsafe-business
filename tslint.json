{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "jsx-no-lambda": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "jsRules": {
    "jsx-no-lambda": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "src/assets/lotties/*.json"
    ]
  }
}
